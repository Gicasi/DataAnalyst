convidados = ["Alice", "Malu", "Paula", "Eloisa", "Gustavo", "Lorenzo", "Ciro", "Marilia", "Ana "]
quantidade = len(convidados)
capacidade = 5
if quantidade > capacidade:
    convidados.remove(convidados[0])
    print("A lista de convidados ultrapassou a capacidade m√°xima. O convidado ", convidados[0], "foi removido da lista.")